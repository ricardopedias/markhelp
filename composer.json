{
    "name": "ricardopedias/markhelp",
    "type": "project",
    "description": "Gerador de páginas HTML para documentações escritars em Markdown",
    "keywords": [
        "help",
        "markdown",
        "documentation"
    ],
    "authors": [
        {
            "name": "Ricardo Pereira Dias",
            "email": "contato@ricardopdias.com.br"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "league/commonmark": "^1.2",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MarkHelp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
