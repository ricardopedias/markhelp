        "sass-loader": "^10.0.1",
        "url-loader": "^4.1.0",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "choices.js": "^9.0.1",
        "highlight.js": "^9.18.1",
        "html5shiv": "^3.7.3",
